{
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node@2.15.10"
    },
    {
      "src": "dist/public/**",
      "use": "@vercel/static@6.3.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ]
}